apiVersion: kops.k8s.io/v1alpha2
kind: InstanceGroup
metadata:
    name: master-c
    labels:
        kops.k8s.io/cluster: clobee.k8s.local
spec:
    role: Master
    minSize: 1
    maxSize: 1
    instanceType: t3.medium
    subnets:
        - clobee-vpc-private-subnet-3
    additionalSecurityGroups:
        - ${TF_VAR_APP_SG_ID}
    iam:
        profile: ${TF_VAR_FLUENTD_PROFILE_ARN}
