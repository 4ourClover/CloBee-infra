apiVersion: kops.k8s.io/v1alpha2
kind: InstanceGroup
metadata:
    name: frontend
    labels:
        kops.k8s.io/cluster: clobee.k8s.local
spec:
    role: Node
    minSize: 1
    maxSize: 1
    instanceType: t3.micro
    subnets:
        - clobee-vpc-public-subnet-1
    additionalSecurityGroups:
        - ${TF_VAR_APP_FE_SG_ID}
